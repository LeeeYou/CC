<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="mIcon" format="reference" />
    <attr name="mColor" format="color" />
    <attr name="mText" format="string" />
    <attr name="mText_size" format="dimension" />

    <declare-styleable name="ChangeColorIconView">
        <attr name="mIcon" />
        <attr name="mColor" />
        <attr name="mText" />
        <attr name="mText_size" />
    </declare-styleable>

    <declare-styleable name="topTitle">

        <attr name="leftText" format="string" />
        <attr name="leftTextSize" format="dimension" />
        <attr name="leftTextColor" format="color" />
        <attr name="leftBackground" format="reference|color" />
        <attr name="leftVisible" format="boolean" />

        <attr name="rightText" format="string" />
        <attr name="rightTextSize" format="dimension" />
        <attr name="rightTextColor" format="reference|color" />
        <attr name="rightBackground" format="reference|color" />
        <attr name="rightVisible" format="boolean" />

        <attr name="centerText" format="string" />
        <attr name="centerTextSize" format="dimension" />
        <attr name="centerTextColor" format="color" />
        <attr name="centerBackground" format="reference|color" />
        <attr name="centerVisible" format="boolean" />

    </declare-styleable>
</resources>